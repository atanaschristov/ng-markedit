{
  "name": "ng-markedit",
  "version": "0.0.1",
  "authors": [
    "athr <atanaschristov@gmail.com>"
  ],
  "description": "angular markdown parser, based on markedown-it, supporting code highlighter and emoji",
  "main": [
    "dist/ng-markedit.js",
    "bower_components/codemirror/theme/base16-light.css",
    "bower_components/emojify.js/dist/css/basic/emojify.css"
  ],
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "angular",
    "markdown",
    "markdown-it",
    "parser",
    "view",
    "review",
    "emoji",
    "highlighter",
    "directive"
  ],
  "license": "MIT",
  "homepage": "https://github.com/atanaschristov/ng-markedit.git",
  "ignore": [
    "**/.*",
    "src",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "^1.4.8",
    "markdown-it": "^5.0.2",
    "markdown-it-footnote": "^2.0.0",
    "highlightjs": "^8.9.1",
    "emojify.js": "^1.1.0"
  },
  "devDependencies": {
    "angular-ui-codemirror": "~0.3.0",
    "angular-mocks": "~1.4.8"
  }
}